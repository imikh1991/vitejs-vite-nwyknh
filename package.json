{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview",
    "lint": "npx eslint src/ --ext .tsx",
    "lint:fix": "npx eslint src/ --ext .tsx --fix",
    "vi:test": "vitest --ui",
    "vi:coverage": "vitest run --coverage",
    "cypress:open": "cypress open --e2e",
    "cy:run-e2e": "cypress run --e2e",
    "coverage": "cypress run --e2e && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/react-router": "^5.1.20",
    "axios": "^1.3.5",
    "babel-plugin-istanbul": "^6.1.1",
    "bootstrap": "^5.2.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "fetch": "^1.1.0",
    "html-react-parser": "^3.0.15",
    "immer": "^10.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-svg": "^16.1.8",
    "ts-node": "^10.9.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress": "^1.1.3",
    "@types/jquery": "^3.5.16",
    "@types/jsdom": "^21.1.1",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.7",
    "@vitest/ui": "^0.29.7",
    "cypress": "^12.10.0",
    "cypress-file-upload": "^5.0.8",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "^4.9.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
